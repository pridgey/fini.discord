{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "outDir": "./dist",
    "target": "ESNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "module": "CommonJS",
    "inlineSourceMap": true
  },
  "exclude": ["node_modules", "ml-models"]
}
