{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "./dist",
    "target": "esnext", // targeting es5 to convert import into require
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "module": "esnext"
  },
  "exclude": ["node_modules", "ml-models"]
}
