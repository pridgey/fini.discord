{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "./dist",
    "target": "es5",
    "esModuleInterop": true,
    "baseUrl": "./",
  },
  "exclude": ["node_modules"]
}
