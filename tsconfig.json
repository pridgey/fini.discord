{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": true,
    "strictNullChecks": false,
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "outDir": "./dist",
    "target": "esnext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "module": "NodeNext",
    "inlineSourceMap": true,
  },
  "exclude": ["node_modules", "ml-models"]
}
