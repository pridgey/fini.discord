{
  "name": "fini.discord",
  "version": "1.0.0",
  "description": "Fini discord bot",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prestart-archived": "./pocketbase serve",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "concurrently \"./pocketbase serve\" \"bun --env-file=.env run index.ts\"",
    "blah": "bun --env-file=.env run index.ts",
    "start-archived": "concurrently \"./pocketbase serve\" \"node --env-file=.env --experimental-specifier-resolution=node --trace-warnings --unhandled-rejections=strict dist/index.js\"",
    "delete-commands": "bun --env-file=.env run command_scripts/delete.ts",
    "register-commands": "bun --env-file=.env run command_scripts/register.ts",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "author": "Taylor Pridgen",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@blockzilla101/citation": "^1.3.0",
    "@discordjs/builders": "latest",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@giphy/js-fetch-api": "^4.1.2",
    "@types/intl": "^1.2.0",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.13.2",
    "canvas": "^3.0.1",
    "date-fns": "^3.2.0",
    "discord-api-types": "latest",
    "discord.js": "latest",
    "dotenv": "^8.2.0",
    "elevenlabs": "^0.16.1",
    "geo-tz": "^8.1.3",
    "gif-encoder-2": "^1.0.5",
    "gif-extract-frames": "^1.0.2",
    "gif-frames": "^1.0.1",
    "gifencoder": "^2.0.1",
    "gifuct-js": "^2.1.2",
    "gifwrap": "^0.10.1",
    "glob": "^10.3.10",
    "gm": "^1.25.0",
    "https": "^1.0.0",
    "i": "^0.3.7",
    "intl": "^1.2.5",
    "jimp": "^1.6.0",
    "lodash.sortby": "^4.7.0",
    "moment": "^2.30.1",
    "node-llama-cpp": "^2.8.5",
    "npm": "^10.4.0",
    "ollama": "^0.5.16",
    "openai": "^4.36.0",
    "pocketbase": "latest",
    "prettier": "^2.7.1",
    "puppeteer": "^24.4.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rcon-client": "^4.2.5",
    "replicate": "^0.32.1",
    "rss-parser": "^3.12.0",
    "seedrandom": "^3.0.5",
    "sharp": "^0.33.5",
    "ts-luxon": "^3.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "^1.0.4",
    "@types/jimp": "^0.2.28",
    "@types/lodash": "^4.17.14",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "@napi-rs/canvas": "0.1.59"
  }
}
